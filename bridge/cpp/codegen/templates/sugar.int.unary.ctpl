#slurp
#compiler-settings
directiveStartToken = %
#end compiler-settings
%slurp

%for $op, $opcode, $optype, $opcount, $typesigs in $data

template <typename T>
inline multi_array<T>& multi_array<T>::$op (const T rhs)
{
    return $opcode.lower() (*this, rhs);
}

template <typename T>
inline multi_array<T>& multi_array<T>::$op (multi_array<T>& rhs)
{
    return $opcode.lower() (*this, rhs);
}
%end for

